# AI Content Auto-Generator Environment Variables
# Copy this file to .env and add your actual API keys

# OpenAI (GPT-4, DALL-E)
OPENAI_API_KEY=sk-your-openai-key-here

# Google Gemini
GEMINI_API_KEY=AIyour-gemini-key-here

# Anthropic (Claude)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# Stability AI
STABILITY_API_KEY=sk-your-stability-key-here

# Adobe Firefly (optional)
ADOBE_API_KEY=your-adobe-key-here

# Redis Configuration (if using Redis cache)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Monitoring
DASHBOARD_PORT=8080

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/generator.log

# Environment
ENVIRONMENT=development  # development, staging, production